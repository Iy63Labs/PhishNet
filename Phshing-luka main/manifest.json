{
  "manifest_version": 3,
  "name": "PhishNet",
  "version": "1.0",
  "description": "Phishing detection tool",
  "permissions": ["tabs", "webRequest", "webNavigation"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon.png",  // Replace with the path to your single icon file
    "default_title": "PhishNet"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["phishing_database.json"],
      "matches": ["<all_urls>"]
    }
  ]
}
