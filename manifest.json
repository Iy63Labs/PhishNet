// manifest.json
{
  "manifest_version": 3,
  "name": "Hello World Extension", #Name of the extension
  "version": "1.0",
  "description": "A simple Chrome extension that displays 'Hello, World!'", #
  #Text description displayed on the chrome store
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [ #sets permissions to access browsers
    "activeTab"
  ]
}
